{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-console": "warn",
    "prefer-const": "error",
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"],
    "indent": ["warn", "tab"],
    "comma-dangle": ["error", "always-multiline"],
    "semi": ["warn", "always"]
  }
}